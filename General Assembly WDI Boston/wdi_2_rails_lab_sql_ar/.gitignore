# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
.env

*.gem
*.rbc
/.config
/coverage/
/InstalledFiles
/pkg/
/spec/reports/
/test/tmp/
/test/version_tmp/


